{
  "scripts": {
    "bundle": "npm run makeCache && npm run webpack && npm run zip",
    "webpack": "webpack",
    "zip": "find . -type f  -not -name '*.js.map' -not -wholename '*/.git/*' -not -wholename '*/node_modules/.cache/*' -not -wholename '*/node_modules/webpack-cli/*' -not -wholename '*/node_modules/webpack/*' -not -wholename '*/node_modules/rxjs/*' -not -wholename '*/node_modules/prettier/*' -not -wholename '*/node_modules/fsevents/*' -not -wholename '*/node_modules/@babel/*' -not -wholename '*/node_modules/webpack-addons/*' -not -wholename '*/node_modules/caniuse-lite/*' | zip -Xr@ lambda.zip",
    "makeCache": "node makeCache"
  },
  "dependencies": {
    "@babel/core": "7.0.0-beta.40",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.40",
    "@babel/polyfill": "7.0.0-beta.40",
    "@babel/preset-env": "7.0.0-beta.40",
    "aws-serverless-express": "3.1.3",
    "babel-loader": "^8.0.0-beta.0",
    "express": "4.16.2",
    "pg": "7.4.1",
    "postgraphile": "4.0.0-beta.2",
    "webpack": "4.1.1",
    "webpack-cli": "2.0.11",
    "webpack-node-externals": "1.6.0"
  }
}
